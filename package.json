{
	"name": "uptime-tui",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"db:generate": "turbo db:generate --filter=@uptime-tui/server",
		"db:migrate": "turbo db:migrate --filter=@uptime-tui/server",
		"db:push": "turbo db:push --filter=@uptime-tui/server",
		"typecheck": "turbo typecheck"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"turbo": "^2.3.3",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"superjson": "^2.2.6"
	},
	"packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
